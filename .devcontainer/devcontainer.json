{
    "name": "workstation-docker-flutter",
    "context": "..",
    "dockerFile": "../Dockerfile",
    "remoteUser": "developer",
    "mounts": ["type=bind"],
    "settings": {
      "terminal.integrated.shell.linux": null
    },
    "runArgs": ["--privileged"],
    "extensions": ["dart-code.flutter","dsznajder.es7-react-js-snippets","msjsdiag.vscode-react-native","esbenp.prettier-vscode"],
    "workspaceMount": "source=${localWorkspaceFolder}/workspace,target=/home/developer/workspace,type=bind,consistency=delegated",
    "workspaceFolder": "/home/developer/workspace"
  }